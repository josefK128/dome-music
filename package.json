{
  "name": "dome-music",
  "version": "1.0.0",
  "description": "music generation application",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "live-server": "live-server --port=8081 --no-browser",
    "createphtrack": "node ./@app/utils/createphtrack",
    "createphtree": "node ./@app/utils/createphtree",
    "initg": "node ./@app/utils/initg",
    "ph2g": "node ./@app/utils/ph2g",
    "g2ph": "node ./@app/utils/g2ph",
    "source2ph": "node ./@app/utils/source2ph",
    "midi2ph": "node ./@app/utils/midi2ph",
    "aut2seq" : "node ./@app/melody/aut2seq",
    "seq2str" : "node ./@app/melody/seq2str",
    "str2abc" : "node ./@app/melody/str2abc",
    "playwav": "node ./@app/audio/playwav",
    "time-pitch": "node ./@app/time-pitch/time-pitch",
    "list": "npm list --depth=0",
    "listg": "npm -g list --depth=0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/josefK128/dome-music.git"
  },
  "author": "rudolph",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/josefK128/dome-music/issues"
  },
  "homepage": "https://github.com/josefK128/dome-music#readme",
  "dependencies": {
    "node-wav-player": "^0.2.0"
  }
}
